    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write \"./src/**/*.{ts,json,html}\"",
    "lint-format": "npm run format &&  ng lint",
    "format-pretty-quick": "pretty-quick --pattern \"src/**/*.*(ts|html|scss)\" --staged",
    "prepare": "husky install",
    "deploy:tag": "sh src/app/core/cicd/deploy-package-json-cicd-tag.sh",
    "post-deploy:version": "node -p \"'export const VERSION_APP = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/core/version/version.ts"
